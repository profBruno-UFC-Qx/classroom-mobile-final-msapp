<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorFundo">#F6F7F9</color>
    <color name="colorPrimary">#006F5F</color>
    <color name="splash_icon_tint">#006F5F</color>
    <string name="app_name">MySpentApp</string>
    <style name="Theme.MySpentApp" parent="Theme.Material3.DayNight.NoActionBar">

        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>
    <style name="Theme.MySpentApp.Splash" parent="Theme.Material3.DayNight.NoActionBar">

        <item name="windowSplashScreenBackground">@color/colorFundo</item>

        <item name="windowSplashScreenAnimatedIcon">@drawable/ms1</item>

        <item name="windowSplashScreenIconBackgroundColor">@color/colorPrimary</item>

        <item name="postSplashScreenTheme">@style/Theme.MySpentApp</item>
    </style>
</resources>